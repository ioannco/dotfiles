alias crm='cd ~/../tttttv/ControlCRM/'
alias crm-bash='sudo docker exec -it controlcrm_web_1 bash'
alias crm-logs='crm; sudo docker-compose logs --tail=3000 -f web'
alias crm-start='sudo docker start controlcrm_web_1'
alias crm-restart='sudo docker restart controlcrm_web_1'
alias crm-stop='sudo docker stop controlcrm_web_1'
alias crm-rebuild-celery='crm; sudo docker-compose up -d --no-deps --build celery celery-beat redis web; sudo docker restart controlcrm_nginx_1'
alias crm-shell='crm; sudo docker-compose exec web python manage.py shell'
alias crm-makemigrations='crm; sudo docker-compose exec web python manage.py makemigrations'
alias crm-migrate='crm; sudo docker-compose exec web python manage.py migrate'
alias crm-test='crm; sudo docker-compose exec web python manage.py test'
alias crm-update-git='crm;sudo git pull origin DEV'
alias crm-update-back='crm-update-git;crm-rebuild-celery'
